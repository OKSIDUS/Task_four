@using Microsoft.AspNetCore.Identity
@inject SignInManager<ApplicationUser> SignInManager
@inject UserManager<ApplicationUser> UserManager

<table class="table">
    <caption>Todo Lists</caption>
    <thead>
        <tr>
            <th>
                ID
            </th>
            <th>
                Name
            </th>
            <th>
                Email
            </th>
            <th>
                LastLogin
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in UserManager.Users)
        {
            <tr>
                <td>
                    @user.Id!
                </td>
                <td>
                    @user.FullName
                </td>
                <td>
                    @user.Email
                </td>
                <td>
                    @user.LastLogin
                </td>
            </tr>
        }
    </tbody>
</table>
